                            <% @today.each do |title, opts| %>
                              <div class="row">
                                <h2 class="text-white"><%= "#{ title } #{ opts[:title] }" %></h2>
                                <% [:wow, :oye].each do |prop| %>
                                  <% if opts.has_key?(prop) %>
                                    <div class="company col-xs-4">
                                      <%= fa_caret opts[prop], text: '% ' + prop.to_s %>
